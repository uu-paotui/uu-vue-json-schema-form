{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "id": "MText",
    "title": "单文本输入组件",
    "description":"单文本输入组件，用于在页面配置一条文字信息" ,
    "type": "object",
    "required": ["txt"],
    "properties": {
        "txt": {
            "title": "文字",
            "type": "string"
        },
        "txtColor": {
            "title": "选择文字颜色",
            "type": "string",
            "default": "#ff0132"
        }
    }
}
